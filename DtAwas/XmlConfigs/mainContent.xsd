          <xs:enumeration value="default"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>

  <xs:attributeGroup name="lv3-section-attrs">
    <xs:attribute name="class" use="optional" default="default">
