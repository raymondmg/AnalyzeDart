          <xs:enumeration value="img_news"/>
          <xs:enumeration value="img_article"/>
          <xs:enumeration value="article"/>
          <xs:enumeration value="default"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>

  <xs:attributeGroup name="lv2-section_article-attrs">
    <xs:attribute name="class" use="optional" default="default">
      <xs:simpleType>
