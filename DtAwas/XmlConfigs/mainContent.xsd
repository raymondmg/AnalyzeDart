        <xs:restriction base="xs:token">
          <xs:enumeration value="col-lg-1"/>
          <xs:enumeration value="col-lg-2"/>
          <xs:enumeration value="col-lg-3"/>
          <xs:enumeration value="col-lg-4"/>
