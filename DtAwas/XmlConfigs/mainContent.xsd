  <xs:attributeGroup name="lv1-section-attrs">
    <xs:attribute name="class" use="optional" default="section">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="home"/>
          <xs:enumeration value="section"/>
        </xs:restriction>
      </xs:simpleType>
